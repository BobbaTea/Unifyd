            <div class="modal-dialog modal-lg modal-dialog-centered modal-dialog-scrollable" role="document">
                <div class="modal-content" style="background-color:rgb(216, 216, 230)">
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-lg-5">
                                <!--Carousel Wrapper-->
                                <div id="carousel-thumbNum" class="carousel slide carousel-fade carousel-thumbnails"
                                    data-ride="carousel">
                                    <!--Slides-->
                                    <div class="carousel-inner" role="listbox">
                                        CarImg
                                    </div>
                                    {{!-- <ol class="carousel-indicators" id="modalcarouselnum">
                                    CarInd
                                </ol> --}}
                                    <a class="carousel-control-prev" href="#carousel-thumbNum" role="button"
                                        data-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carousel-thumbNum" role="button"
                                        data-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </div>
                                <!--/.Carousel Wrapper-->
                            </div>
                            <div class="col-lg-7">
                                <h2 class="h2-responsive product-name">
                                    <strong><input class="form-control form-control-lg" type="text" value="productName">
                                    </strong>
                                </h2>
                                categories
                                <h4 class="h4-responsive">
                                    <span class="green-text">
                                        <strong><input class="form-control" type="text" value="priceStart">
                                            - <input class="form-control" type="text" value="priceEnd">
                                        </strong>
                                    </span>
                                </h4>
                                <p><input class="form-control" type="text" value="description"></p>

                                <div class="card-body align-text-bottom">
                                    <div class="row col-md-12">
                                        <div class="form-group col-md-6 ">
                                            <label for="">Price</label>
                                            <input type="number" class="form-control" min="priceStart" max="priceEnd">
                                        </div>
                                        <div class="form-group col-md-6 ">
                                            <label for="">Quantity (limit max)</label>
                                            <input type="number" class="form-control" min="1" max="limit"
                                                onchange="validate()">
                                        </div>
                                    </div>
                                    <script>

                                    </script>
                                    <div class="text-center">
